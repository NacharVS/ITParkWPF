<Window x:Class="ITParkApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ITParkApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="33*"/>
            <ColumnDefinition Width="167*"/>
        </Grid.ColumnDefinitions>
        <Image x:Name="image" Margin="0,294,0,36"/>
        <Button x:Name="button" Content="Button" Margin="0,0,22,144" Click="button_Click_1"/>
        <Button x:Name="warrior" Content="Warrior" Grid.Column="1" HorizontalAlignment="Left" Height="37" Margin="134,290,0,0" VerticalAlignment="Top" Width="99" Click="warrior_Click"/>
        <Button x:Name="rogue" Content="Rogue" Grid.Column="1" HorizontalAlignment="Left" Height="37" Margin="255,290,0,0" VerticalAlignment="Top" Width="95" Click="rogue_Click"/>
        <Button x:Name="sorcerer" Content="Sorcerer" Grid.Column="1" HorizontalAlignment="Left" Height="37" Margin="379,290,0,0" VerticalAlignment="Top" Width="109" Click="sorcerer_Click"/>

    </Grid>
</Window>
